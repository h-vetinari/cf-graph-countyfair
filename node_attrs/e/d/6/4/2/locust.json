 "linux_64_meta_yaml": {
  "about": {
   "description": "Locust is an easy-to-use, distributed, user load testing tool. It is\nintended for load-testing web sites (or other systems) and figuring out how\nmany concurrent users a system can handle.",
   "dev_url": "https://github.com/locustio/locust",
   "doc_url": "https://docs.locust.io/",
   "home": "https://locust.io/",
   "license": "MIT",
   "license_family": "MIT",
   "license_file": "dist/LICENSE",
   "summary": "Website load testing framework"
  },
  "build": {
   "noarch": "python",
   "number": "0",
   "script": {
    "content": [
     "cd dist",
     "rm uv.lock || del uv.lock",
     "${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation --disable-pip-version-check"
    ],
    "env": {
     "SKIP_PRE_BUILD": "true"
    }
   }
  },
  "extra": {
   "recipe-maintainers": [
    "carlodri",
    "bollwyvl"
   ]
  },
  "outputs": [
   {
    "build": null,
    "name": "locust",
    "requirements": {
     "build": [],
     "host": [
      "hatch-vcs",
      "hatchling",
      "pip",
      "python 3.9.*"
     ],
     "run": [
      "configargparse >=1.5.5",
      "flask >=2.0.0",
      "flask-cors >=3.0.10",
      "flask-login >=0.6.3",
      "gevent >=24.10.1",
      "geventhttpclient >=2.3.1",
      "msgpack-python >=1.0.0",
      "psutil >=5.9.1",
      "python >=3.9",
      "pywin32-on-windows",
      "pyzmq >=25.0.0",
      "requests >=2.32.2",
      "tomli >=1.1.0",
      "typing_extensions >=4.6.0",
      "werkzeug >=2.0.0"
     ]
    },
    "tests": [
     {
      "python": {
       "imports": [
        "locust"
       ],
       "python_version": "3.9.*"
      }
     },
     {
      "files": {
       "recipe": [
        "run_test.py"
       ],
       "source": [
        "src/locust/test/**"
       ]
      },
      "requirements": {
       "run": [
        "cryptography",
        "mock",
        "pip",
        "pyquery",
        "pytest-timeout",
        "python 3.9.*",
        "retry"
       ]
      },
      "script": [
       "locust --version",
       "locust --help",
       "python -c \"import locust; assert locust.__version__ == '${{ version }}'\"",
       "pip list | grep -iE \"^locust\\s*${{ version | replace('.', '\\\\.') }}$\"",
       "locust --version | grep -iE \"${{ version | replace('.', '\\\\.') }}\"",
       "python run_test.py"
      ]
     }
    ]
   }
  ],
