 "linux_64_meta_yaml": {
  "about": {
   "description": "OneLOop is a program to evaluate the one-loop scalar 1-point, 2-point,\n3-point and 4-point functions, for all kinematical configurations relevant\nfor collider-physics, and for any non-positive imaginary parts of the\ninternal squared masses. It deals with all UV and IR divergences within\ndimensional regularization. Furthermore, it provides routines to evaluate\nthese functions using straightforward numerical integration.\n",
   "dev_url": "https://bitbucket.org/hameren/oneloop/",
   "doc_url": "https://helac-phegas.web.cern.ch/OneLOop.html",
   "home": "https://bitbucket.org/hameren/oneloop/",
   "license": "GPL-3.0-only",
   "license_family": "GPL",
   "license_file": "COPYING",
   "summary": "OneLOop: A library of one-loop scalar functions"
  },
  "build": {
   "number": "1",
   "run_exports": [
    "oneloop"
   ],
   "script": [
    "sed -i \"s|FC = gfortran|FC = $FC|g\" Config",
    "sed -i \"s|FFLAGS = -O|FFLAGS = $FFLAGS|g\" Config",
    "python create.py dynamic",
    "mkdir -p $PREFIX/include/oneloop",
    "mv *.mod $PREFIX/include/oneloop/",
    "mv libavh_olo${SHLIB_EXT} $PREFIX/lib/",
    "./clean.sh"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "matthewfeickert"
   ]
  },
