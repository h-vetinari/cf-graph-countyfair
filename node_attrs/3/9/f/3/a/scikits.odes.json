 "linux_64_meta_yaml": {
  "about": {
   "description": "Odes is a scikit toolkit for scipy to add extra ode solvers. Specifically\nit interfaces the Sundials solvers cvode, cvodes, ida and idas. It this way\nit provides extra modern ode and dae solvers you can use, extending the\ncapabilities offered in scipy.integrade.ode.\n",
   "dev_url": "https://github.com/bmcage/odes",
   "doc_url": "https://scikits-odes.readthedocs.io",
   "home": "https://github.com/bmcage/odes",
   "license": "BSD-3-Clause",
   "license_family": "BSD",
   "license_file": "LICENSE.txt",
   "summary": "A python module for ordinary differential equation anddifferential algebraic equation solvers"
  },
  "build": {
   "number": "2",
   "script": [
    "PYTHON -m pip install -vv --no-dependencies --no-build-isolation scikits-odes-core/",
    "PYTHON -m pip install -vv --no-dependencies --no-build-isolation scikits-odes-sundials/",
    "MESON_ARGS_REDUCED=\"$(echo $MESON_ARGS | sed 's/--buildtype release //g')\"",
    "PYTHON -m pip install -vv --no-dependencies --no-build-isolation scikits-odes-daepack/ -Csetup-args=${MESON_ARGS_REDUCED// / -Csetup-args=}",
    "PYTHON -m pip install --no-dependencies --no-build-isolation . -vv"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "moorepants"
   ]
  },
  "package": {
   "name": "scikits.odes",
   "version": "3.0.0"
  },
  "requirements": {
   "build": [
    "c_compiler_stub",
    "fortran_compiler_stub",
    "c_stdlib_stub"
   ],
   "host": [
    "cython <3",
    "libblas",
    "meson-python",
    "more-itertools",
    "numpy",
    "pip",
    "pkgconfig",
    "python",
    "setuptools",
    "sundials >=6.0.0,<7"
   ],
   "run": [
    "numpy",
    "python",
    "scipy",
    "sundials >=6.0.0,<7"
   ]
  },
