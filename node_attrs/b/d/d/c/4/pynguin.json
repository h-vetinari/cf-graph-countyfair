  "about": {
   "description": "Unit test automated generation techniques have been proposed and mature\n tools exist for statically typed languages, such as Java. Pynguin is on of\n the first tool that fills this gap and allows the automated generation of\n unit tests for Python programs.\n",
   "doc_url": "https://pynguin.readthedocs.io/en/latest/",
   "home": "https://github.com/se2p/pynguin",
   "license": "LGPL-3.0-only",
   "license_file": "LICENSE.rst",
   "summary": "Pynguin is a tool for automated unit test generation for Python"
  },
  "build": {
   "entry_points": [
    "pynguin = pynguin.cli:main"
   ],
   "noarch": "python",
   "number": "1",
   "script": "PYTHON -m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "thewchan"
   ]
  },
  "package": {
   "name": "pynguin",
   "version": "0.40.0"
  },
  "requirements": {
   "host": [
    "pip",
    "poetry-core",
    "python 3.10"
   ],
   "run": [
    "python >=3.10,<3.11",
    "astroid >=3.3.5,<4.0.0",
    "black >=24.10.0,<25.0.0",
    "bytecode >=0.16.0,<0.17.0",
    "jellyfish >=1.1.0,<2.0.0",
