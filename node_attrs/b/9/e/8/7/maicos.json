  "about": {
   "description": "MAICoS is an object-oriented python toolkit for analysing the structure and dynamics of interfacial and confined fluids from molecular simulations. Combined with MDAnalysis, MAICoS can be used to extract density profiles, dielectric constants, structure factors, or transport properties from trajectories files, including LAMMPS, GROMACS, CHARMM or NAMD data. MAICoS is open source and is released under the GNU general public license v3.0.",
   "dev_url": "https://gitlab.com/maicos-devel/maicos",
   "doc_url": "https://www.maicos-analysis.org",
   "home": "https://www.maicos-analysis.org",
   "license": "GPL-3.0-or-later",
   "license_family": "GPL",
   "license_file": "LICENSE",
   "summary": "Analyse molecular dynamics simulations of interfacial and confined systems."
  },
  "build": {
   "entry_points": [
    "maicos = maicos.__main__:main"
   ],
   "number": "0",
   "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
  },
