   "number": "1",
   "run_exports": [
    "siscone"
   ],
   "script": [
    "cp $BUILD_PREFIX/share/gnuconfig/config.* .",
    "autoreconf --install",
    "./configure --help",
    "bash autogen.sh --prefix=$PREFIX --disable-static",
    "make --jobs=\"${CPU_COUNT}\"",
    "make check",
    "make install",
    "make clean"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "kratsg",
    "matthewfeickert"
   ]
  },
  "package": {
   "name": "siscone",
   "version": "3.0.6"
  },
  "requirements": {
   "build": [
    "c_stdlib_stub",
    "cxx_compiler_stub",
    "automake",
    "libtool",
    "make",
    "pkg-config",
    "gnuconfig"
   ]
  },
  "schema_version": 0,
  "source": {
   "patches": [
    "0001-remove-os-libtoolize-assumptions.patch"
   ],
   "sha256": "fd852e5ae3349a1ad540e49b1dd7efe3a188c3f7591902de4aeb781c421ddf13",
