      "test -f $PREFIX/lib/libomp.so",
      "export LNK_XTRA=\"-Wl,--allow-shlib-undefined\"",
      "$PREFIX/bin/clang -v -fopenmp -I$PREFIX/include -L$PREFIX/lib -Wl,-rpath,$PREFIX/lib $LNK_XTRA omp_hello.c -o omp_hello",
      "./omp_hello"
     ],
     "files": [
      "omp_hello.c"
     ],
     "requires": [
      "clangxx",
      "cxx_compiler_stub"
     ]
    }
   },
   {
